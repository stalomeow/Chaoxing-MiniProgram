/* components/watermark/watermark.wxss */
/* 想要改水印大小或者稀疏通过样式调整即可 */
.water_top {
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	transform: rotate(-10deg);
	z-index: 9;
}

.water-text {
	float: left;
	width: 375rpx;
	color: rgba(0, 0, 0, 0.15);
	text-align: center;
	font-size: 15px;
	margin: 120rpx 0;
}